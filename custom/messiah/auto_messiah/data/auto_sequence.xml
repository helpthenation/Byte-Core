<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences For auto.mechanic (Auto Mechanic) -->

        <record id="seq_auto_mechanic" model="ir.sequence">
            <field name="name">Auto Mechanic</field>
            <field name="code">auto.mechanic</field>
            <field name="prefix">AM-SP-%(year)s%(month)s</field>
            <field name="padding">5</field>
        </record>
        <!-- Sequences For auto.towing.resource (Auto Towing Resource) -->

        <record id="seq_auto_mechanic" model="ir.sequence">
            <field name="name">Auto Towing Resource</field>
            <field name="code">auto.towing.resource</field>
            <field name="prefix">AM-TS-%(year)s%(month)s</field>
            <field name="padding">5</field>
        </record>
        <!-- Sequences For auto.service (Auto Service) -->

        <record id="seq_auto_service" model="ir.sequence">
            <field name="name">Auto Service</field>
            <field name="code">auto.service</field>
            <field name="prefix">SRV-%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
        </record>

        <!-- Sequences For auto.towing (Auto Towing) -->

        <record id="seq_auto_towing" model="ir.sequence">
            <field name="name">Auto Towing</field>
            <field name="code">auto.towing</field>
            <field name="prefix">TOW-%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
        </record>

         <!-- Sequences For auto.payment (Auto Payment) -->
        <record id="seq_auto_payment" model="ir.sequence">
            <field name="name">Auto Payment</field>
            <field name="code">auto.payment</field>
            <field name="prefix">PAY-%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
