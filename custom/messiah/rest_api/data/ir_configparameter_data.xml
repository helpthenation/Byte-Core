<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!-- OAuth2 tokens time to live: -->
<record id="oauth2_access_token_expires_in" model="ir.config_parameter">
    <field name="key">oauth2_access_token_expires_in</field>
    <field name="value">600</field>
</record>
<record id="oauth2_refresh_token_expires_in" model="ir.config_parameter">
    <field name="key">oauth2_refresh_token_expires_in</field>
    <field name="value">7200</field>
</record>

<!-- Redis server settings: -->
<record id="redis_host" model="ir.config_parameter">
    <field name="key">redis_host</field>
    <field name="value">localhost</field>
</record>
<record id="redis_port" model="ir.config_parameter">
    <field name="key">redis_port</field>
    <field name="value">6379</field>
</record>
<record id="redis_db" model="ir.config_parameter">
    <field name="key">redis_db</field>
    <field name="value">0</field>
</record>
<record id="redis_password" model="ir.config_parameter">
    <field name="key">redis_password</field>
    <field name="value">None</field>
</record>

    </data>
</openerp>
