<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_hr_employee_configuration" model="ir.ui.view">
            <field name="name">hr.employee.config.settings.inherit</field>
            <field name="model">hr.employee.config.settings</field>
            <field name="inherit_id"
                   ref="hr_employee_config_setting.view_hr_employee_configuration"/>
            <field name="arch" type="xml">
                <header position="after">
                    <group>
                        <label for="id" string="Contract Interval"/>
                        <div>
                            <div>
                                <field name="contract_interval"
                                       class="oe_inline"/>
                                <label for="contract_interval"
                                       class="oe_inline"/>

                            </div>
                        </div>
                    </group>

                </header>
            </field>
        </record>
    </data>
</odoo>
